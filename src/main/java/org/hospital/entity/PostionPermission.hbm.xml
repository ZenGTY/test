<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.hospital.entity.PostionPermission" table="postionpermission" schema="" catalog="hospitalsystem">
        <composite-id mapped="true" class="org.hospital.entity.PostionPermissionPK">
            <key-property name="positionId">
                <column name="positionId" sql-type="int unsigned" not-null="true"/>
            </key-property>
            <key-property name="permissionId">
                <column name="permissionId" sql-type="int unsigned" not-null="true"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>