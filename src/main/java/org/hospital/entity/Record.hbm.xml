<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.hospital.entity.Record" table="record" schema="" catalog="hospitalsystem">
        <id name="recordId">
            <column name="recordId" sql-type="bigint unsigned" not-null="true"/>
        </id>
        <property name="employeeId">
            <column name="employeeId" sql-type="bigint unsigned" not-null="true"/>
        </property>
        <property name="clientId">
            <column name="clientId" sql-type="bigint unsigned" not-null="true"/>
        </property>
        <property name="complain">
            <column name="complain" sql-type="varchar" length="500"/>
        </property>
        <property name="presentIllness">
            <column name="presentIllness" sql-type="varchar" length="500"/>
        </property>
        <property name="pastIllness">
            <column name="pastIllness" sql-type="varchar" length="500"/>
        </property>
        <property name="physicalExamination">
            <column name="physicalExamination" sql-type="varchar" length="500"/>
        </property>
        <property name="basis">
            <column name="basis" sql-type="varchar" length="500"/>
        </property>
        <property name="antidiastole">
            <column name="antidiastole" sql-type="varchar" length="500"/>
        </property>
        <property name="preliminaryDiagnosis">
            <column name="preliminaryDiagnosis" sql-type="varchar" length="500"/>
        </property>
        <property name="diagnosisPlan">
            <column name="diagnosisPlan" sql-type="varchar" length="500"/>
        </property>
        <property name="treatmentPlan">
            <column name="treatmentPlan" sql-type="varchar" length="500"/>
        </property>
        <property name="createTime">
            <column name="createTime" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>