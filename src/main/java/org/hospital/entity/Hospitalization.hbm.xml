<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.hospital.entity.Hospitalization" table="hospitalization" schema="" catalog="hospitalsystem">
        <id name="hospitalizationId">
            <column name="hospitalizationId" sql-type="bigint unsigned" not-null="true"/>
        </id>
        <property name="clientId">
            <column name="clientId" sql-type="bigint unsigned" not-null="true"/>
        </property>
        <property name="departmentId">
            <column name="departmentId" sql-type="int unsigned" not-null="true"/>
        </property>
        <property name="codeIcd">
            <column name="codeICD" sql-type="varchar" length="50"/>
        </property>
        <property name="enterDiagnosis">
            <column name="enterDiagnosis" sql-type="varchar"/>
        </property>
        <property name="leaveDiagnosis">
            <column name="leaveDiagnosis" sql-type="varchar"/>
        </property>
        <property name="enterSituation">
            <column name="enterSituation" sql-type="varchar"/>
        </property>
        <property name="leaveSituation">
            <column name="leaveSituation" sql-type="varchar"/>
        </property>
        <property name="enterTime">
            <column name="enterTime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="leaveTime">
            <column name="leaveTime" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>