<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.hospital.entity.Log" table="log" schema="" catalog="hospitalsystem">
        <id name="logId">
            <column name="logId" sql-type="bigint unsigned" not-null="true"/>
        </id>
        <property name="employeeId">
            <column name="employeeId" sql-type="bigint unsigned" not-null="true"/>
        </property>
        <property name="content">
            <column name="content" sql-type="varchar" length="500" not-null="true"/>
        </property>
        <property name="operate">
            <column name="operate" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="operateTime">
            <column name="operateTime" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>