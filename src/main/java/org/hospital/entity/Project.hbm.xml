<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.hospital.entity.Project" table="project" schema="" catalog="hospitalsystem">
        <id name="projectId">
            <column name="projectId" sql-type="bigint unsigned" not-null="true"/>
        </id>
        <property name="creatorId">
            <column name="creatorId" sql-type="bigint unsigned" not-null="true"/>
        </property>
        <property name="category">
            <column name="category" sql-type="varchar" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="unitPrice">
            <column name="unitPrice" sql-type="int unsigned" not-null="true"/>
        </property>
        <property name="introduction">
            <column name="introduction" sql-type="varchar"/>
        </property>
        <property name="suggestNumber">
            <column name="suggestNumber" sql-type="smallint unsigned" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="smallint unsigned" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="smallint unsigned" not-null="true"/>
        </property>
        <property name="createTime">
            <column name="createTime" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>