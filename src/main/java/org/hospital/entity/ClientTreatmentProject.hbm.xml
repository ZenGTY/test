<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.hospital.entity.ClientTreatmentProject" table="clienttreatmentproject" schema=""
           catalog="hospitalsystem">
        <id name="clientTreatmentProjectId">
            <column name="clientTreatmentProjectId" sql-type="bigint unsigned" not-null="true"/>
        </id>
        <property name="clientId">
            <column name="clientId" sql-type="bigint unsigned" not-null="true"/>
        </property>
        <property name="billId">
            <column name="billId" sql-type="bigint unsigned" not-null="true"/>
        </property>
        <property name="projectId">
            <column name="projectId" sql-type="bigint unsigned" not-null="true"/>
        </property>
        <property name="unitPrice">
            <column name="unitPrice" sql-type="double unsigned" precision="22" not-null="true"/>
        </property>
        <property name="totalNumber">
            <column name="totalNumber" sql-type="smallint unsigned" not-null="true"/>
        </property>
        <property name="restNumber">
            <column name="restNumber" sql-type="smallint unsigned" not-null="true"/>
        </property>
        <property name="startTime">
            <column name="startTime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="deadline">
            <column name="deadline" sql-type="date" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="smallint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>